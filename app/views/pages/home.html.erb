  <!--Pie chart script-->
  <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
  <script type="text/javascript">
    google.charts.load('current', {'packages':['corechart']});
    google.charts.setOnLoadCallback(drawChart);

    function drawChart() {

      var data = google.visualization.arrayToDataTable([
        ['Grupo', 'Categoria (lbs)'],
        ['<  250',     <%= @wl250 %>],
        ['250 - 500',      <%= @w250 %>],
        ['500 - 750',  <%= @w500 %>],
        ['750 - 1,000', <%= @w750 %>],
        ['> 1,000',    <%= @w1000 %>]
      ]);

      var options = {
        pieSliceText: 'percentage'
      };

      var chart = new google.visualization.PieChart(document.getElementById('bovine-piechart'));

      chart.draw(data, options);
    }
  </script>
  <script type="text/javascript">
    google.charts.load('current', {'packages':['corechart']});
    google.charts.setOnLoadCallback(drawChart);

    function drawChart() {

      var data = google.visualization.arrayToDataTable([
        ['Grupo', 'Categoria (lbs)'],
        ['<  25',     <%= @owl25 %>],
        ['25 - 50',      <%= @ow25 %>],
        ['50 - 75',  <%= @ow50 %>],
        ['75 - 100', <%= @ow75 %>],
        ['> 100',    <%= @ow100 %>]
      ]);

      var options = {
        pieSliceText: 'percentage'
      };

      var chart = new google.visualization.PieChart(document.getElementById('ovine-piechart'));

      chart.draw(data, options);
    }
  </script>
